{
    "build":[
        {
            "src":"index.js",
            "use":"@vercel/node"
        },
        {
            "src":"build/**",
            "use":"@vercel/static"
        }
    ],
    "routes":[
        {
            "src":"/qwertyuiop",
            "dest":"index.js"
        },
        {
            "src":"/auth/signin",
            "dest":"index.js"
        },
        {
            "src":"/auth/signup",
            "dest":"index.js"
        },
        {
            "src":"/",
            "dest":"build/index.html"
        },
        {
            "src":"/(.+)",
            "dest":"build/$1"
        },
        {
            "src":"/(.+)",
            "dest":"build/index.html"
        }
    ]
}